{
  "name": "<%= appname %>",
  "version": "0.0.0",
  "description": "App written with TypeScript and Angular linked with JSPM.",
  "scripts": {
    "test": "karma start",
    "postinstall": "jspm install && gulp"
  },
  "keywords": [
    "angular",
    "jspm",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= ghUser %>/<%= appname %>.git"
  },
  "author": "<%= ghUser %>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/<%= ghUser %>/<%= appname %>/issues"
  },
  "homepage": "https://github.com/<%= ghUser %>/<%= appname %>#readme",
  "jspm": {
    "directories": {},
    "configFile": "jspm.config.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.5",
      "angular-decorators": "github:hourliert/angular-decorators@^0.1.0",
      "css": "github:systemjs/plugin-css@^0.1.16",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "core-js": "npm:core-js@^0.9.4",
      "typescript": "npm:typescript@^1.6.0-dev.20150818"
    }
  },
  "devDependencies": {
    "browser-sync": "^2.9.3",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-bump": "^0.3.1",
    "gulp-git": "^1.4.0",
    "gulp-html-replace": "^1.5.1",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-tag-version": "^1.3.0",
    "gulp-tsd": "0.0.4",
    "gulp-tslint": "^3.3.0-beta",
    "gulp-typedoc": "^1.2.1",
    "gulp-uglify": "^1.4.1",
    "jspm": "^0.16.2",
    "jspm-bower-endpoint": "^0.3.2",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "run-sequence": "^1.1.2",
    "tsd": "^0.6.4"
  }
